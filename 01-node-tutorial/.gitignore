# Ignore sensitive or environment-specific information
.env
config.js
secret.key

# Exclude specific files
path/to/file.txt

# Exclude specific directories
path/to/directory/

# Exclude all files with a specific extension
*.extension

# Exclude all files in a specific directory with a specific extension
path/to/directory/*.extension
